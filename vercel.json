{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/leaderboard",
      "destination": "/fitrack/index.html"
    },
    {
      "source": "/challenges",
      "destination": "/fitrack/index.html"
    },
    {
      "source": "/admin",
      "destination": "/fitrack/index.html"
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}